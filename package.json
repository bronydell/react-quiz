{
  "name": "exan-mobile",
  "version": "0.0.1",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "open:android": "react-native-scripts android",
    "open:ios": "react-native-scripts ios",
    "lint:js": "eslint ./const ./src",
    "lint:styles": "stylelint './src/**/*.js'",
    "lint": "yarn lint:js && yarn lint:styles",
    "test": "node node_modules/jest/bin/jest.js",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'"
  },
  "dependencies": {
    "expo": "^25.0.0",
    "firebase": "^4.13.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "~16.2.0",
    "react-native": "0.52.0",
    "react-native-dotenv": "^0.1.1",
    "react-navigation": "^1.5.8",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2",
    "babel-plugin-module-resolver": "^3",
    "babel-plugin-transform-react-pug": "^5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-pug": "^0.2.0",
    "jest-expo": "25.0.0",
    "react-dom": "^16.2.0",
    "react-native-scripts": "~1.11.1",
    "react-test-renderer": "~16.2.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/dist/styled-components.native.cjs.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "./config/setupTests.js"
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "stylelint": {
    "extends": "./config/stylelint.js"
  }
}
